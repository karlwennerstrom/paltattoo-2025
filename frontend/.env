# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_VERSION=v1

# Environment
NODE_ENV=development
REACT_APP_ENV=development

# Application Configuration
REACT_APP_NAME=PalTattoo
REACT_APP_DESCRIPTION=Plataforma que conecta clientes con tatuadores profesionales
REACT_APP_VERSION=1.0.0

# URLs
REACT_APP_BASE_URL=http://localhost:3000
REACT_APP_WEBSITE_URL=https://paltattoo.com

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp
REACT_APP_IMAGES_BASE_URL=http://localhost:5000/uploads

# Google Services (replace with your actual keys)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Social Media Links
REACT_APP_FACEBOOK_URL=https://facebook.com/paltattoo
REACT_APP_INSTAGRAM_URL=https://instagram.com/paltattoo
REACT_APP_TWITTER_URL=https://twitter.com/paltattoo
REACT_APP_LINKEDIN_URL=https://linkedin.com/company/paltattoo

# Payment Configuration (Stripe - replace with your keys)
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_SUBSCRIPTIONS=true
REACT_APP_ENABLE_GOOGLE_AUTH=true
REACT_APP_ENABLE_ANALYTICS=false

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here
REACT_APP_FACEBOOK_PIXEL_ID=your_facebook_pixel_id_here

# Sentry Error Tracking
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Pagination
REACT_APP_ARTISTS_PER_PAGE=12
REACT_APP_TATTOOS_PER_PAGE=24
REACT_APP_REVIEWS_PER_PAGE=10

# Cache Configuration
REACT_APP_CACHE_TIMEOUT=300000

# Map Configuration
REACT_APP_DEFAULT_LAT=-33.4489
REACT_APP_DEFAULT_LNG=-70.6693
REACT_APP_DEFAULT_ZOOM=12

# Email Configuration
REACT_APP_SUPPORT_EMAIL=support@paltattoo.com
REACT_APP_CONTACT_EMAIL=contact@paltattoo.com

# Regional Settings
REACT_APP_DEFAULT_LANGUAGE=es
REACT_APP_DEFAULT_CURRENCY=CLP
REACT_APP_DEFAULT_TIMEZONE=America/Santiago

# Development Tools
REACT_APP_DEBUG_MODE=true
REACT_APP_SHOW_PERFORMANCE_METRICS=true

# Security
REACT_APP_ENABLE_HTTPS=false
REACT_APP_COOKIE_DOMAIN=localhost

# Content Security Policy
REACT_APP_CSP_REPORT_URI=https://your-domain.com/csp-report

# Image Processing
REACT_APP_IMAGE_QUALITY=80
REACT_APP_THUMBNAIL_SIZE=300

# Notification Settings
REACT_APP_NOTIFICATION_TIMEOUT=5000
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true

# Search Configuration
REACT_APP_SEARCH_DEBOUNCE_TIME=300
REACT_APP_MIN_SEARCH_LENGTH=2

# Rate Limiting
REACT_APP_API_RATE_LIMIT=100
REACT_APP_API_RATE_WINDOW=3600

# Session Configuration
REACT_APP_SESSION_TIMEOUT=1800000
REACT_APP_SESSION_WARNING=300000

# Backup Configuration
REACT_APP_BACKUP_API_URL=http://localhost:5000/api

# Testing
REACT_APP_MOCK_API=false
REACT_APP_TEST_MODE=false